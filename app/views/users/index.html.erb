<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Salutation</th>
      <th>Email</th>
      <th>Phone number</th>
      <th>User type</th>
      <th>Vat number</th>
      <th>Active</th>
      <th>Account</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.salutation %></td>
        <td><%= user.email %></td>
        <td><%= user.phone_number %></td>
        <td><%= user.user_type %></td>
        <td><%= user.vat_number %></td>
        <td><%= user.active %></td>
        <td><%= user.account %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <!-- deleted destroy function -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
